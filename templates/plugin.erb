# File managed by Puppet

LoadPlugin <%= @name -%>

<Plugin <%= @name -%>>
    <% @config_file_options_hash.each { |key, value|
        if ! value.is_a?(Array) -%>
               <%= "#{key}\t\t#{value}" %>
        <% else
             value.each { |subvalue| %>
                <%= "#{key}\t\t#{subvalue}" %>
        <%   }
           end -%>

    <% } -%>

</Plugin>
